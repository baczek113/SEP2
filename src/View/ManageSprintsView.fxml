<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="420.0" prefWidth="681.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.ManageSprintsViewController">
    <children>
        <Button fx:id="addSprint" layoutX="541.0" layoutY="87.0" mnemonicParsing="false" onAction="#add" prefHeight="26.0" prefWidth="115.0" text="Add" />
        <Button fx:id="editSprint" layoutX="541.0" layoutY="130.0" mnemonicParsing="false" onAction="#edit" prefHeight="26.0" prefWidth="115.0" text="Edit" />
        <Button fx:id="removeSprint" layoutX="541.0" layoutY="170.0" mnemonicParsing="false" onAction="#remove" prefHeight="26.0" prefWidth="115.0" text="Remove" />
        <TableView fx:id="tableView" layoutY="64.0" prefHeight="350.0" prefWidth="520.0">
            <columns>
                <TableColumn fx:id="name" prefWidth="173.0" text="Name" />
                <TableColumn fx:id="startDate" prefWidth="173.0" text="Start date" />
                <TableColumn fx:id="endDate" prefWidth="173.0" text="End date" />
            </columns>
        </TableView>
        <Button fx:id="infoSprint" layoutX="541.0" layoutY="210.0" mnemonicParsing="false" onAction="#info" prefHeight="26.0" prefWidth="115.0" text="View" />
        <Label layoutX="220.0" layoutY="14.0" prefHeight="47.0" prefWidth="240.0" text="Manage Sprints" textAlignment="CENTER">
            <font>
                <Font size="32.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
