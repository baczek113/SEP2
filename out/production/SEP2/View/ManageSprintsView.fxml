<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="466.0" prefWidth="681.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.ManageSprintsViewController">
    <children>
        <Button fx:id="addSprint" layoutX="116.0" layoutY="426.0" mnemonicParsing="false" onAction="#add" prefHeight="26.0" prefWidth="115.0" text="Add" />
        <Button fx:id="editSprint" layoutX="287.0" layoutY="426.0" mnemonicParsing="false" onAction="#edit" prefHeight="26.0" prefWidth="115.0" text="Edit" />
        <Button fx:id="removeSprint" layoutX="465.0" layoutY="426.0" mnemonicParsing="false" onAction="#remove" prefHeight="26.0" prefWidth="115.0" text="Remove" />
        <TableView fx:id="tableView" layoutY="64.0" prefHeight="350.0" prefWidth="689.0">
            <columns>
                <TableColumn fx:id="name" prefWidth="230.0" text="Name" />
                <TableColumn fx:id="startDate" prefWidth="230.0" text="Start date" />
                <TableColumn fx:id="endDate" prefWidth="230.0" text="End date" />
            </columns>
        </TableView>
        <Label layoutX="225.0" layoutY="14.0" prefHeight="47.0" prefWidth="240.0" text="Manage Sprints" textAlignment="CENTER">
            <font>
                <Font size="32.0" />
            </font>
        </Label>
      <Button fx:id="goBack" layoutX="20.0" layoutY="24.0" mnemonicParsing="false" onAction="#goBack" prefHeight="8.0" prefWidth="34.0" text="←" />
    </children>
</AnchorPane>
